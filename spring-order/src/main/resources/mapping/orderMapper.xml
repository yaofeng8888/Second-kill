<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yf.springorder.mapper.IOrder">
    <insert id="insertOrder" parameterType="com.yf.springorder.model.Order">
        insert into orderservice (id,createtime,status,username)values (
        #{id},#{timestamp},#{status},#{name});
    </insert>
</mapper>